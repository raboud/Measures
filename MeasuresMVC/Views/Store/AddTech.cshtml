@using RandREng.MeasureDBEntity
@model MeasuresMVC.Models.AddTech

@{
	ViewBag.Title = "Assign Tech";
}

<h2>Create</h2>


@using (Html.BeginForm())
{
	@Html.AntiForgeryToken()

	<div class="form-horizontal">
		<h4>Store</h4>
		<hr />
		@Html.ValidationSummary(true)

		<div class="form-group">
			@Html.LabelFor(model => model.TechId, "TechId", new { @class = "control-label col-md-2" })
			<div class="col-md-10">
				@Html.DropDownList("TechId", String.Empty)
				@Html.ValidationMessageFor(model => model.TechId)
			</div>
		</div>

		<div class="form-group">
			<div class="col-md-offset-2 col-md-10">
				<input type="submit" value="Assign" class="btn btn-default" />
			</div>
		</div>
	</div>
}

<div>
	@Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
	@Scripts.Render("~/bundles/jqueryval")
}
